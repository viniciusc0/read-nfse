{
  "name": "xmlservico",
  "version": "1.0.0",
  "description": "Leitura de XMLS de NFSe - Municipios",
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"ts-node --transpile-only ./src/server.ts\"",
    "test": "nodemon --exec \"cross-env NODE_ENV=test mocha -r ts-node/register ./src/**/*.test.ts --exit || exit 1\"",
    "build:documentation": "copyfiles --flat ./src/documentation/openapi.json ./dist/documentation/",
    "build": "tsc && npm run build:documentation"
  },
  "dependencies": {
    "@types/amqplib": "^0.8.2",
    "amqplib": "^0.8.0",
    "cli-highlight": "2.1.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "fs": "0.0.1-security",
    "nodemon": "^2.0.12",
    "swagger-ui-dist": "3.44.1",
    "xml-js": "^1.6.11",
    "xml2json": "^0.12.0"
  },
  "private": true,
  "devDependencies": {
    "copyfiles": "2.3.0",
    "cross-env": "7.0.2",
    "mocha": "8.1.3",
    "node-mocks-http": "1.9.0",
    "nodemon": "2.0.4",
    "sequelize-cli": "6.2.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3",
    "swagger-ui-dist": "3.44.1"
  }
}
